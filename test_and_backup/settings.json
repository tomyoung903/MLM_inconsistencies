    "multiCommand.commands": [
        {
            "command": "InsertTextRunCellTrue",
            "sequence": [
                {
                    "command": "type",
                    "args": {
                        "text": "RUN_CELL = True "
                    }
                }
            ]
        },
        {
            "command": "InsertTextFalse",
            "sequence": [
                {
                    "command": "type",
                    "args": {
                        "text": "False"
                    }
                }
            ]
        },
        {
            "command": "InsertTextTrue",
            "sequence": [
                {
                    "command": "type",
                    "args": {
                        "text": "True"
                    }
                }
            ]
        },
        {
            "command": "InsertTextRunCellFalse",
            "sequence": [
                {
                    "command": "type",
                    "args": {
                        "text": "RUN_CELL = False"
                    }
                }
            ]
        },
        {
            "command": "cursorMove8LinesDown",
            "sequence": [
                {
                    "command": "cursorMove",
                    "args": {
                        "to": "down",
                        "by": "line",
                        "value": 8
                    },
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "command": "cursorMove8LinesUp",
            "sequence": [
                {
                    "command": "cursorMove",
                    "args": {
                        "to": "up",
                        "by": "line",
                        "value": 8
                    },
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "command": "cursorMove100charRight",
            "sequence": [
                {
                    "command": "cursorMove",
                    "args": {
                        "to": "right",
                        "by": "character",
                        "value": 100
                    },
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "command": "cursorMove1000LinesUp",
            "sequence": [
                {
                    "command": "cursorMove",
                    "args": {
                        "to": "up",
                        "by": "line",
                        "value": 1000
                    },
                    "when": "editorTextFocus"
                }
            ]
        },   
        // delete right 16 chars
        {
            "command": "deleteRight16Chars",
            "sequence": [
                {

                    "command": "repeat-commands.runSequence",
                    "args": {
                            "commands": [                            // these will be repeated
                            "deleteRight",
                        ],
                        "repeat": 16,   // hard-coded amount to repeat the 'commands``
                    },
                    "when": "editorTextFocus"
                }
            ]
        },
        // delete right 4 chars
